{
	"name": "@markuplint/cli-utils",
	"version": "4.4.11",
	"description": "Utilities for CLI of Markuplint",
	"repository": "git@github.com:markuplint/markuplint.git",
	"author": "Yusuke Hirao <yusukehirao@me.com>",
	"license": "MIT",
	"private": false,
	"type": "module",
