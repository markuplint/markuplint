{
	"name": "@markuplint/types",
	"version": "3.5.1",
	"description": "Type declaration and value checker",
	"repository": "git@github.com:markuplint/markuplint.git",
	"author": "Yusuke Hirao <yusukehirao@me.com>",
	"license": "MIT",
	"private": false,
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"typedoc": {
		"entryPoint": "./src/index.ts"
	},
	"scripts": {
		"build": "tsc",
		"clean": "tsc --build --clean",
		"schema": "ts-node './gen/types.ts'; json2ts './types.schema.json' > './src/types.schema.ts'; prettier './src/types.schema.ts' './types.schema.json' --write; eslint './src/types.schema.ts' --fix; tsc;"
	},
	"dependencies": {
		"bcp-47": "1",
		"css-tree": "^2.3.1",
		"debug": "^4.3.4",
		"leven": "3",
		"whatwg-mimetype": "3"
	},
	"devDependencies": {
		"@types/bcp-47": "1",
		"@types/css-tree": "^2.0.1",
		"@types/debug": "^4.1.7",
		"@types/whatwg-mimetype": "3",
		"type-fest": "^3.8.0"
	}
}
