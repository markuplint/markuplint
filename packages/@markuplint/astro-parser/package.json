		"clean": "tsc --build --clean tsconfig.build.json"
	},
	"dependencies": {
		"@markuplint/ml-ast": "4.4.10",
		"@markuplint/parser-utils": "4.8.8",
		"astro-eslint-parser": "1.2.2"
	},
	"devDependencies": {
		"@astrojs/compiler": "2.12.2"
	}
