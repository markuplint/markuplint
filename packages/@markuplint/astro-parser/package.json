{
	"name": "@markuplint/astro-parser",
	"version": "4.6.20",
	"description": "astro parser for markuplint",
	"repository": "git@github.com:markuplint/markuplint.git",
	"author": "Yusuke Hirao <yusukehirao@me.com>",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"import": "./lib/index.js",
			"types": "./lib/index.d.ts"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
