{
	"name": "@markuplint/create-rule",
	"version": "4.7.14",
	"description": "Rule generator for markuplint",
	"repository": "git@github.com:markuplint/markuplint.git",
	"author": "Yusuke Hirao <yusukehirao@me.com>",
	"license": "MIT",
	"private": false,
	"type": "module",
	"exports": {
		".": {
			"import": "./lib/index.js",
			"types": "./lib/index.d.ts"
		}
	},
	"bin": {
		"create-rule": "./bin/create-rule.mjs"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsc --project tsconfig.build.json",
		"dev": "tsc --watch --project tsconfig.build.json",
		"clean": "tsc --build --clean --project tsconfig.build.json"
	},
	"dependencies": {
		"@markuplint/cli-utils": "4.4.10",
		"@markuplint/ml-core": "4.11.0",
		"glob": "11.0.0",
		"prettier": "3.4.2",
		"ts-node": "10.9.2",
		"typescript": "5.7.3"
	},
	"devDependencies": {
		"@types/fs-extra": "11.0.4",
		"fs-extra": "11.2.0"
	}
}
