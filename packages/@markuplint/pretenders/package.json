{
	"name": "@markuplint/pretenders",
	"version": "0.0.16",
	"description": "It loads components and then creates the pretenders data from them.",
	"repository": "git@github.com:markuplint/markuplint.git",
	"author": "Yusuke Hirao <yusukehirao@me.com>",
	"license": "MIT",
	"private": false,
	"type": "module",
