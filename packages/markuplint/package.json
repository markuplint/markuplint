		"@markuplint/ml-core": "4.0.0-alpha.8",
		"@markuplint/ml-spec": "4.0.0-alpha.8",
		"@markuplint/rules": "4.0.0-alpha.8",
		"@markuplint/shared": "4.0.0-alpha.8",
		"@types/debug": "^4.1.12",
		"chokidar": "^3.5.3",
